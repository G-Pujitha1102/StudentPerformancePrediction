ggplot(metrics, aes(x = Model, y = RMSE, fill = Model)) +
geom_bar(stat = "identity") +
theme_minimal() +
ggtitle("RMSE Comparison of Models")
# RÂ² Comparison Plot
ggplot(metrics, aes(x = Model, y = R2, fill = Model)) +
geom_bar(stat = "identity") +
theme_minimal() +
ggtitle("RÂ² Comparison of Models")
